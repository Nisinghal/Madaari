<% include ./partials/header %>
<div class="workspace program">
  <% include ./partials/story-access %> <% include ./partials/panel %>
  <div class="board">
    <div class="block trash" aria-label="trash">
      <div class="can">
        <i class="fas fa-bars fa-sm"></i>
        <div class="lid"></div>
      </div>
    </div>
  </div>
  <div class="music-cue-panel">
    <div class="panel-title">Note Sequence Generator</div>
    <div class="close-panel"><i class="fas fa-times"></i></div>
    <div class="panel-content">
      <div class="record-button"><p>Record</p></div>
      <div class="music-notes">Sequence</div>
    </div>
    <button class="save-notes-button disabled">Use Sequence as Cue</button>
  </div>
  <div class="consoles">
    <% include ./partials/music-console %> <% include ./partials/animation %> <%
    include ./partials/animatronics %>
  </div>
</div>
